<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/navStyle.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Template/SBAdmin2/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Template/SBAdmin2/vendor/metisMenu/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Template/SBAdmin2/dist/css/sb-admin-2.css" rel="stylesheet" />
    <link href="~/Template/SBAdmin2/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/navStyle.css" rel="stylesheet" />
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
   

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>



    @Html.Partial("header");
    @Html.Partial("TopNav");
    @RenderBody()
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
    </footer>


    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryswal")*@
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
